{
    "name": "ttrpg-environment",
    "version": "a.0.22",
    "description": "A plugin development template for devs who want to use svelte in Obsidian.",
    "main": "main.js",
    "type": "module",
    "jest": {
        "preset": "ts-jest",
        "globals": {
            "ts-jest": {
                "isolatedModules": true
            }
        },
        "collectCoverage": true,
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "html"
        ],
        "testEnvironment": "node",
        "moduleFileExtensions": [
            "js",
            "ts"
        ],
        "transform": {
            "^.+\\.(ts)$": "ts-jest"
        },
        "testMatch": [
            "**/*.test.ts",
            "**/*.test.js"
        ]
    },
    "scripts": {
        "style": "sass ./modules/styles.scss styles.css",
        "lint": "eslint . --ext .ts",
        "dev": "vite build --watch",
        "vite": "vite build --watch",
        "build": "vite build",
        "bumpversion": "node version-bump.mjs && git add manifest.json versions.json",
        "test": "jest --coverage"
    },
    "keywords": [],
    "author": "Grobax",
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^1.0.1",
        "@tsconfig/svelte": "^3.0.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^16.18.87",
        "builtin-modules": "3.3.0",
        "jest": "^29.7.0",
        "obsidian": "latest",
        "sass": "^1.60.0",
        "svelte": "^3.49.0",
        "svelte-check": "^2.8.0",
        "svelte-preprocess": "^4.10.7",
        "ts-jest": "^29.2.5",
        "tslib": "^2.6.2",
        "typescript": "^4.9.5",
        "vite": "^3.0.0"
    },
    "dependencies": {
        "async-mutex": "^0.5.0",
        "grobax-json-handler": "github:GrobaxGrappleBlast/GrobaxJSONHandler",
        "jszip": "^3.10.1",
        "yaml": "^2.5.0"
    }
}
